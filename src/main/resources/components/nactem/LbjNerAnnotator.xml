        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>Features</name>
        <description>Which features to use</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>Labels</name>
        <description>Which labels to output</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TokenizationScheme</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
