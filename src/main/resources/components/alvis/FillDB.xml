<alvisnlp-doc author="Robert Bossy" beta="yes" date="2012-04-30" short-target="FillDB" target="org.bibliome.alvisnlp.modules.FillDB">
  <synopsis>
    <p>Stores the corpus into a SQL database.</p>
  </synopsis>

  <module-doc>
    <description>
      <p/>
    </description>
  
    <param-doc mandatory="required" name="password" short-type="String" type="java.lang.String">
      <p>Password for RDBMS access.</p>
    </param-doc>
  
    <param-doc mandatory="required" name="username" short-type="String" type="java.lang.String">
      <p>User for RDBMS access.</p>
    </param-doc>
  
    <param-doc mandatory="default: true" name="documentFilter" short-type="Expression" type="alvisnlp.corpus.expressions.Expression">
      <p>Only process document that satisfy this filter.</p>
    </param-doc>
  
    <param-doc mandatory="default: true" name="active" short-type="Expression" type="alvisnlp.corpus.expressions.Expression">
      <p>This expression is evaluated as a boolean with the corpus as the context element, the module will process the document only if the result is true.</p>
    </param-doc>
      
    <param-doc mandatory="required" name="schema" short-type="String" type="java.lang.String">
      <p>Schema of the filled tables.</p>
    </param-doc>
  
    <param-doc mandatory="default: true" name="sectionFilter" short-type="Expression" type="alvisnlp.corpus.expressions.Expression">
      <p>Process only sections that satisfy this filter.</p>
    </param-doc>
  
    <param-doc mandatory="default: org.postgresql.Driver" name="jdbcDriver" short-type="String" type="java.lang.String">
      <p>JDBC driver for the RDBMS.</p>
    </param-doc>
  
    <param-doc mandatory="required" name="url" short-type="String" type="java.lang.String">
      <p>URL of the database.</p>
    </param-doc>
  </module-doc>
</alvisnlp-doc>
