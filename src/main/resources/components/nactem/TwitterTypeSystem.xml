<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>TwitterTypeSystem</name>
  <description/>
  <version>0.0.1-SNAPSHOT</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.Tweet</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>createdAt</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>source</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>truncated</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>user</name>
          <description/>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>place</name>
          <description/>
          <rangeTypeName>uk.ac.nactem.uima.twitter.Place</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>contributors</name>
          <description/>
          <rangeTypeName>uima.cas.LongArray</rangeTypeName>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>coordinate</name>
          <description/>
          <rangeTypeName>uk.ac.nactem.uima.twitter.Coordinate</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>entities</name>
          <description/>
          <rangeTypeName>uk.ac.nactem.uima.twitter.Entities</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>inReplyToStatusId</name>
          <description/>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>inReplyToUserId</name>
          <description/>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.Coordinate</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>longitude</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>latitude</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.Entities</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>hashtagEntities</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.twitter.HashTagEntity</elementType>
        </featureDescription>
        <featureDescription>
          <name>userMentionEntities</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.twitter.UserMentionEntity</elementType>
        </featureDescription>
        <featureDescription>
          <name>mediaEntities</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.twitter.MediaEntity</elementType>
        </featureDescription>
        <featureDescription>
          <name>urlEntities</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.twitter.URLEntity</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.MediaEntity</name>
      <description/>
      <supertypeName>uk.ac.nactem.uima.twitter.URLEntity</supertypeName>
      <features>
        <featureDescription>
          <name>mediaUrl</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>mediaUrlHttps</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>mediaType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.Place</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>country</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>countryCode</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>fullName</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>placeType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>url</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>boundingBox</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.twitter.CoordinateList</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.URLEntity</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>displayURL</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>expandedURL</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>url</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.HashTagEntity</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.UserMentionEntity</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>screenName</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.twitter.CoordinateList</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>coordinates</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.twitter.Coordinate</elementType>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
