<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>Oscar3TypeSystemDescriptor</name>
  <description>This type system implements the data structure required for chemical entity annotations spewed out by the Oscar3 system.</description>
  <version>1.0</version>
  <vendor/>
  <imports>
    <import name="org.u_compare.U_compareTypeSystem"/>
  </imports>
  <types>
    <typeDescription>
      <name>uk.ac.nactem.cheta.ChemicalEntity</name>
      <description>This is the parent type subclassing the various classes generated by the Oscar 3</description>
      <supertypeName>org.u_compare.shared.semantic.NamedEntity</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.cheta.CM</name>
      <description>Chemical compound</description>
      <supertypeName>uk.ac.nactem.cheta.ChemicalEntity</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.cheta.RN</name>
      <description>Reaction</description>
      <supertypeName>uk.ac.nactem.cheta.ChemicalEntity</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.cheta.CPR</name>
      <description>Chemical prefix</description>
      <supertypeName>uk.ac.nactem.cheta.ChemicalEntity</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.cheta.CJ</name>
      <description>Chemical adjectives</description>
      <supertypeName>uk.ac.nactem.cheta.ChemicalEntity</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.cheta.ASE</name>
      <description>enzyme</description>
      <supertypeName>uk.ac.nactem.cheta.ChemicalEntity</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.cheta.ONT</name>
      <description>Ontology</description>
      <supertypeName>uk.ac.nactem.cheta.ChemicalEntity</supertypeName>
    </typeDescription>
  </types>
</typeSystemDescription>
