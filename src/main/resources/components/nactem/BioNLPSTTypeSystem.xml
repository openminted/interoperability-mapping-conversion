<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>BioNLP Shared Task Type System</name>
  <description/>
  <version>0.2</version>
  <vendor/>
  <imports>
    <import name="uk.ac.nactem.uima.EventTypeSystem"/>
  </imports>
  <types>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.bionlpst.Event</name>
      <description/>
      <supertypeName>uk.ac.nactem.uima.cas.semantic.NamedEvent</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>negation</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>speculation</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>notes</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.cas.bionlpst.Note</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.bionlpst.Entity</name>
      <description/>
      <supertypeName>uk.ac.nactem.uima.cas.semantic.NamedAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>notes</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.cas.bionlpst.Note</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.bionlpst.EquivalentEntities</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>entities</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.cas.bionlpst.Entity</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.bionlpst.Relation</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>arguments</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.cas.semantic.NamedEventParticipant</elementType>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>uk.ac.nactem.uima.cas.bionlpst.Note</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>noteID</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>noteType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>targetID</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>noteContent</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
