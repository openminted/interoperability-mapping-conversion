<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>org.u_compare.analysis_engine.banner.BannerCRFTagger</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>BANNER CRF Tagger</name>
    <description>A UIMA wrapper for BANNER entity tagger. BANNER uses CRF.</description>
    <version>1.0</version>
    <vendor>Arizona State University/U-Tokyo/U-Compare</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>TypeToBioSuffixMap</name>
        <description>Mappings from BIO suffix to the UIMA type names.</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>UseNumericNormalization</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>UseParenthesisPostProcessing</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>ModelFile</name>
        <description>File location of CRF trained model generated by BANNER, abstract path is recommended. If not specified, BANNER's default model is used.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>TypeToBioSuffixMap</name>
        <value>
          <array>
            <string>org.u_compare.shared.semantic.bio.Protein,protein</string>
            <string>org.u_compare.shared.semantic.bio.Protein,Protein</string>
            <string>org.u_compare.shared.semantic.bio.Protein,PROTEIN</string>
            <string>org.u_compare.shared.semantic.bio.Gene,gene</string>
            <string>org.u_compare.shared.semantic.bio.Gene,Gene</string>
            <string>org.u_compare.shared.semantic.bio.Gene,GENE</string>
            <string>org.u_compare.shared.semantic.bio.DNA,DNA</string>
            <string>org.u_compare.shared.semantic.bio.RNA,RNA</string>
            <string>org.u_compare.shared.semantic.bio.CellLine,CellLine</string>
            <string>org.u_compare.shared.semantic.bio.CellType,CellType</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>UseNumericNormalization</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>UseParenthesisPostProcessing</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="org.u_compare.U_compareTypeSystem"/>
      </imports>
    </typeSystemDescription>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.u_compare.shared.syntactic.Sentence</type>
          <type allAnnotatorFeatures="true">org.u_compare.shared.syntactic.RichToken</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">org.u_compare.shared.semantic.bio.RNA</type>
          <type allAnnotatorFeatures="true">org.u_compare.shared.semantic.bio.Protein</type>
          <type allAnnotatorFeatures="true">org.u_compare.shared.semantic.bio.Gene</type>
          <type allAnnotatorFeatures="true">org.u_compare.shared.semantic.bio.DNA</type>
          <type allAnnotatorFeatures="true">org.u_compare.shared.semantic.bio.CellType</type>
          <type allAnnotatorFeatures="true">org.u_compare.shared.semantic.bio.CellLine</type>
        </outputs>
        <languagesSupported>en</languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
