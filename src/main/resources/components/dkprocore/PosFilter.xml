<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
    <primitive>true</primitive>
    <annotatorImplementationName>de.tudarmstadt.ukp.dkpro.core.posfilter.PosFilter</annotatorImplementationName>
    <analysisEngineMetaData>
        <name>de.tudarmstadt.ukp.dkpro.core.posfilter.PosFilter</name>
        <description>Removes all tokens/lemmas/stems/POS tags (depending on the "Mode" setting) that do not match the
given parts of speech.</description>
        <version>1.8.0-SNAPSHOT</version>
        <vendor>DKPro Core Project</vendor>
        <copyright>Copyright 2010-2015
                            Ubiquitous Knowledge Processing (UKP) Lab
                            Technische Universit√§t Darmstadt</copyright>
        <configurationParameters>
            <configurationParameter>
                <name>typeToRemove</name>
                <description>The fully qualified name of the type that should be filtered.</description>
                <type>String</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>adj</name>
                <description>Keep/remove adjectives (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>adv</name>
                <description>Keep/remove adverbs (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>art</name>
                <description>Keep/remove articles (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>card</name>
                <description>Keep/remove cardinal numbers (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>conj</name>
                <description>Keep/remove conjunctions (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>n</name>
                <description>Keep/remove nouns (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>o</name>
                <description>Keep/remove "others" (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>pp</name>
                <description>Keep/remove prepositions (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>pr</name>
                <description>Keep/remove pronouns (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>punc</name>
                <description>Keep/remove punctuation (true: keep, false: remove)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
            <configurationParameter>
                <name>Verbs</name>
                <description>Keep/remove verbs (true: keep, false: v)</description>
                <type>Boolean</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
        </configurationParameters>
        <configurationParameterSettings>
            <nameValuePair>
                <name>adj</name>
                <value>
                    <boolean>false</boolean>
                </value>
            </nameValuePair>
            <nameValuePair>
                <name>adv</name>
