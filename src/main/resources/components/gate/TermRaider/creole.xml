ions" DEFAULT="false">java.lang.Boolean</PARAMETER><PARAMETER NAME="scoreProperty" DEFAULT="pmiScore" COMMENT="name of main score">java.lang.String</PARAMETER><PARAMETER NAME="debugMode" COMMENT="print debugging information during initialization" DEFAULT="false">java.lang.Boolean</PARAMETER><PARAMETER NAME="inputASName" COMMENT="input AS name" DEFAULT="">java.lang.String</PARAMETER><PARAMETER NAME="languageFeature" COMMENT="language feature on term candidates" DEFAULT="lang">java.lang.String</PARAMETER><PARAMETER NAME="inputAnnotationFeature" COMMENT="input annotation feature" DEFAULT="canonical">java.lang.String</PARAMETER><PARAMETER NAME="corpora" ITEM_CLASS_NAME="gate.Corpus" COMMENT="Processed corpora to analyse">java.util.Set</PARAMETER></RESOURCE><RESOURCE><CLASS>gate.termraider.bank.TfIdfTermbank</CLASS><NAME>TfIdfTermbank</NAME><COMMENT>TermRaider Termbank derived from vectors in document features</COMMENT><HELPURL>http://gate.ac.uk/userguide/sec:creole:termraider:tfidf</HELPURL><ICON>termbank-lr.png</ICON><PARAMETER NAME="normalization" COMMENT="score normalization" DEFAULT="Sigmoid">gate.termraider.modes.Normalization</PARAMETER><PARAMETER NAME="scoreProperty" DEFAULT="tfIdf" COMMENT="name of main score">java.lang.String</PARAMETER><PARAMETER NAME="docFreqSource" COMMENT="document frequency bank (unset = create from these corpora)" OPTIONAL="true">gate.termraider.bank.DocumentFrequencyBank</PARAMETER><PARAMETER NAME="tfCalculation" COMMENT="term frequency calculation" DEFAULT="Logarithmic">gate.termraider.modes.TfCalculation</PARAMETER><PARAMETER NAME="idfCalculation" COMMENT="inverted document frequency calculation" DEFAULT="LogarithmicScaled">gate.termraider.modes.IdfCalculation</PARAMETER><PARAMETER NAME="inputAnnotationTypes" ITEM_CLASS_NAME="java.lang.String" COMMENT="input annotation types" DEFAULT="SingleWord;MultiWord">java.util.Set</PARAMETER><PARAMETER NAME="idDocumentFeature" COMMENT="doc feature to use for identification (blank = use sourceURL)